#carousel-who.slide.carousel(data-ride="carousel-who")
  .carousel-inner

      .item.active
        img(src="images/who-we-are/who_intro.jpg", alt="...")

      .item
        div.row
          div.col-md-4.who-image-box
            img(src="images/who-we-are/tyler.jpg")

          div.col-md-4.who-image-box
            img(src="images/who-we-are/kate.jpg")

          div.col-md-4.who-image-box
            img(src="images/who-we-are/dara.jpg")

          div.col-md-4.who-image-box
            img(src="images/who-we-are/patima.jpg")

          div.col-md-4.who-image-box
            img(src="images/who-we-are/dayman.jpg")

          div.col-md-4.who-image-box
            img(src="images/who-we-are/patima.jpg")

  <!-- Controls -->
  a.left.carousel-control(href="who-we-are/#carousel-who", data-slide="prev")
    span.glyphicon.glyphicon-chevron-left
  a.right.carousel-control(href="who-we-are/#carousel-who", data-slide="next")
    span.glyphicon.glyphicon-chevron-right
